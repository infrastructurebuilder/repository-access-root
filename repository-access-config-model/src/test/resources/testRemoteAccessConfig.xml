<?xml version="1.0" encoding="UTF-8"?>
<repositoryAccessConfiguration>
  <modelVersion>1.0.0</modelVersion>
  <name>RootConfig</name>
  <description>A set of root configurations</description>
  <instances>
    <instance>
      <id>default</id>
      <localRepo>target/repo/default</localRepo>
      <remoteRepo>http://us.infrastructurebuilder.com/maven2/</remoteRepo>
      <credentials>
        <serverId>someServer</serverId>
      </credentials>
      <inclusions>
        <inclusion>
          <types>
            <type>
              <classifier>prod</classifier>
              <extension>somext</extension>
            </type>
          </types>
          <scopes>
            <scope>prod</scope>
          </scopes>
        </inclusion>
      </inclusions>
      <exclusions>
        <exclusion>
          <types>
            <type>
              <classifier>testing</classifier>
              <extension>otherext</extension>
            </type>
          </types>
          <scopes>
            <scope>test</scope>
          </scopes>
        </exclusion>
      </exclusions>
    </instance>
    <instance>
      <id>elsewhere</id>
      <localRepo>target/repo/elsewhere</localRepo>
      <remoteRepo>http://central.maven.org/maven2/</remoteRepo>
      <credentials>
        <principal>user</principal>
        <secret>password</secret>
      </credentials>
      <inclusions>
        <inclusion>
          <types>
            <type>
              <classifier>testing</classifier>
              <extension>somext</extension>
            </type>
          </types>
          <scopes>
            <scope>test</scope>
          </scopes>
        </inclusion>
      </inclusions>
      <exclusions>
        <exclusion>
          <types>
            <type>
              <classifier>prod</classifier>
              <extension>otherext</extension>
            </type>
          </types>
          <scopes>
            <scope>prod</scope>
          </scopes>
        </exclusion>
      </exclusions>
    </instance>
  </instances>
</repositoryAccessConfiguration>